{
  "name": "type-ahead",
  "version": "0.1.0",
  "main": "dist/type-ahead.umd.js",
  "module": "dist/type-ahead.esm.js",
  "unpkg": "dist/type-ahead.min.js",
  "browser": {
    "./sfc": "src/components/provider-type-ahead.vue"
  },
  "private": true,
  "scripts": {
    "serve-cli": "vue-cli-service serve",
    "build-cli": "vue-cli-service build",
    "lint-cli": "vue-cli-service lint",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/type-ahead.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/type-ahead.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/type-ahead.min.js"
  },
  "dependencies": {
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "rollup": "^1.20.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.1.6",
    "rollup-plugin-vue2": "^0.8.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
